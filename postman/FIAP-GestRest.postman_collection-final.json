		{
			"name": "Cadastro Valido",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Comprehensive post-request test script for: Cadastro Valido",
							"(() => {",
							"  try {",
							"    // 1. Verify response status is 2xx (POST endpoint expects 201 Created)",
							"    pm.test('Status is successful (2xx)', () => {",
							"      pm.expect(pm.response.code).to.be.within(200, 299);",
							"    });",
							"",
							"    // 2. Response time under 2000ms",
							"    pm.test('Response time is under 2000ms', () => {",
							"      pm.expect(pm.response.responseTime).to.be.below(2000);",
							"    });",
							"",
							"    // 3. Valid JSON when Content-Type is application/json",
							"    const contentType = pm.response.headers.get('Content-Type') || '';",
							"    if (contentType.includes('application/json')) {",
							"      pm.test('Response is valid JSON', () => {",
							"        pm.response.to.be.json;",
							"      });",
							"    }",
							"",
							"    // 4. POST endpoint specific assertions - expects 201 Created and response contains id",
							"    const method = pm.request.method.toUpperCase();",
							"    if (method === 'POST') {",
							"      pm.test('POST created resource with id', () => {",
							"        pm.expect(pm.response.code).to.be.oneOf([200, 201, 202]);",
							"        if (contentType.includes('application/json')) {",
							"          const json = pm.response.json();",
							"          pm.expect(json).to.have.property('id');",
							"          pm.collectionVariables.set('lastCreatedId', json.id);",
							"        }",
							"      });",
							"    }",
							"  } catch (err) {",
							"    console.log('Test script error:', err);",
							"  }",
							"})();"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
